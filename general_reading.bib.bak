@Article{Lehre2020,
  author  = {Per Kristian Lehre and Dirk Sudholt},
  journal = {IEEE transaction on evolutionary computation},
  title   = {Parallel Black-Box Complexity With Tail Bounds},
  year    = {2020},
  number  = {6},
  pages   = {1010-1024},
  volume  = {24},
  comment = {This research gives a baseline of how many evaluations are needed at least to compare different algorithms.

This research claims that for EA the generation of next population is of parallel nature, unlike  traditional unerstanding, which says it is sequential nature.

Good expression:
Using a large offspring population in many cases can decrease the number of generation needed to find an optimal solution. However,  the number of function evaluations may increase as evolution can only act on information from the previous generation. A large offspring popultion can lead to inertia that slows down the optimization process.},
}

@Article{Doerr2020,
